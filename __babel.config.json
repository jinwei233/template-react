{
  "presets": [
    [
      "@babel/preset-env", {
        "targets": "ios >= 8, android >= 4.4, >= 1%",
        "loose": true,
        "useBuiltIns": "entry",
        "corejs": 3
      }
    ],
    "@babel/preset-react"
  ],
  "plugins": [
    "@babel/plugin-proposal-class-properties",
    "@babel/plugin-transform-runtime",
    "@babel/plugin-transform-async-to-generator"
  ]
}
